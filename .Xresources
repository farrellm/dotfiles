! my customisations
XTerm*faceName: Droid Sans Mono
XTerm*faceSize: 8
xterm*metaSendsEscape:      true

UXTerm*faceName: Droid Sans Mono
UXTerm*faceSize: 8
Uxterm*metaSendsEscape:      true

URxvt.scrollBar: false
URxvt*font: xft:Droid Sans Mono:pixelsize=12:antialias=true:hinting=true
!URxvt*font: xft:Ubuntu Mono:pixelsize=12:antialias=true:hinting=true

xpdf*background: grey

! Zenburn color-scheme
! urxvt*background:  #3f3f3f
urxvt*background:  #3c3c3c
urxvt*foreground:  #dcdccc
! black + red
!urxvt*color0:     #3f3f3f
urxvt*color0:      #1E2320
! urxvt*color1:      #705050
urxvt*color1:      #7890a8
! green + yellow
urxvt*color2:      #60b48a
urxvt*color3:      #dfaf8f
! blue + purple
urxvt*color4:      #506070
urxvt*color5:      #dc8cc3
! cyan + white
urxvt*color6:      #8cd0d3
urxvt*color7:      #dcdccc
! bright-black + bright-red
urxvt*color8:      #709080
urxvt*color9:      #dca3a3
! bright-green + bright-yellow
urxvt*color10:     #c3bf9f
urxvt*color11:     #f0dfaf
! bright-blue + bright-purple
urxvt*color12:     #94bff3
urxvt*color13:     #ec93d3
! bright-cyan + bright-white
urxvt*color14:     #93e0e3
urxvt*color15:     #ffffff

